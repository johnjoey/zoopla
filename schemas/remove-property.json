{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "title" : "listing/delete",
   "type" : "object",
   "additionalProperties" : false,
   "id" : "http://realtime-listings.webservices.zpg.co.uk/docs/v2.3/schemas/listing/delete.json",
   "properties" : {
      "listing_reference" : {
         "type" : "string",
         "pattern" : "^\\S(|(.|\\n)*\\S)\\Z"
      },
      "deletion_reason" : {
         "enum" : [
            "withdrawn",
            "offer_accepted",
            "exchanged",
            "completed",
            "let"
         ]
      }
   },
   "required" : [
      "listing_reference"
   ]
}

